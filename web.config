<configuration>
    <system.webServer>
        <handlers><add name="iisnode" path="server.js" verb="*" modules="iisnode"/></handlers>
        <rewrite>
            <rules>
              <rule name="DynamicContent">
                   <conditions>
                        <add input="{{REQUEST_FILENAME}}" matchType="IsFile" negate="True"/>
                   </conditions>
                   <action type="Rewrite" url="server.js"/>
              </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
